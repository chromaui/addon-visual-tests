    "filesize": "^10.0.12",
    "jsonfile": "^6.1.0",
    "react-confetti": "^6.1.0",
    "strip-ansi": "^7.1.0"
  },
  "description": "Visual Testing addon with Chromatic",
  "devDependencies": {
    "@emotion/weak-memoize": "^0.3.1",
    "@graphql-codegen/cli": "^4.0.1",
